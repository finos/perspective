        "eslint-plugin-prettier": "^2.6.2",
        "file-loader": "^2.0.0",
        "graceful-fs": "https://github.com/mekwall/node-graceful-fs.git#patch-1",
        "html-loader": "^0.5.1",
        "html-loader-jest": "^0.2.1",
        "inquirer": "^7.0.0",
        "jest": "^24.5.0",
        "js-beautify": "^1.8.6",
