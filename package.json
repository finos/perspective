        "base64-font-loader": "0.0.4",
        "chalk": "^2.4.2",
        "clean-css-cli": "4.2.1",
        "clean-css-loader": "^1.1.0",
        "codemirror": "^5.30.0",
        "codemirror-javascript": "^0.2.0",
        "core-js": "^2.6.9",
